@font-face {
  font-family: 'TitilliumText22L-Bold';
  src: url('TitilliumText22L-Bold.woff') format('woff'),
       url('TitilliumText22L-Bold.ttf') format('truetype'),
       url('TitilliumText22L-Bold.svg#TitilliumText22L-Bold') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'TitilliumText22L005-webfont';
  src: url('TitilliumText22L005-webfont.eot');
  src: url('TitilliumText22L005-webfont.eot?#iefix') format('embedded-opentype'),
       url('TitilliumText22L005-webfont.woff2') format('woff2');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'TitilliumText22L-Regular';
  src: url('TitilliumText22L-Regular.woff') format('woff'),
       url('TitilliumText22L-Regular.ttf') format('truetype'),
       url('TitilliumText22L-Regular.svg#TitilliumText22L-Regular') format('svg');
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'TitilliumText22L003-webfont';
  src: url('TitilliumText22L003-webfont.eot');
  src: url('TitilliumText22L003-webfont.eot?#iefix') format('embedded-opentype'),
       url('TitilliumText22L003-webfont.woff2') format('woff2');
  font-weight: normal;
  font-style: normal;
}

